# Environment Variables Template for Production
# Copy this file to .env.local and fill in your actual values

# IBM Quantum API Configuration
IBM_QUANTUM_API_TOKEN=your_ibm_quantum_api_token_here
IBM_QUANTUM_CHANNEL=ibm_quantum
IBM_QUANTUM_HUB_GROUP_PROJECT=ibm-q/open/main

# Example:
# IBM_QUANTUM_API_TOKEN=abc123def456...
# IBM_QUANTUM_CHANNEL=ibm_quantum
# IBM_QUANTUM_HUB_GROUP_PROJECT=ibm-q/open/main

# Next.js Configuration
NEXT_PUBLIC_APP_NAME="Quantum Circuit Simulator"
NEXT_PUBLIC_APP_VERSION="1.0.0"
NEXTAUTH_SECRET=your_secure_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# For Production Deployment
# NEXTAUTH_URL=https://your-domain.vercel.app

# Optional: Database (if needed for job persistence)
# DATABASE_URL=your_database_url_here

# Optional: Analytics and Monitoring
# NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id
# SENTRY_DSN=your_sentry_dsn

# Deployment Configuration
NODE_ENV=development
